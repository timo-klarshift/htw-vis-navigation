package logging

import org.apache.log4j.ConsoleAppender;
import org.apache.log4j.Logger

class VisLog {
	static {
		initLogging()
	}
	
	static void initLogging(){
		Logger logger = Logger.getRootLogger()
		ConsoleAppender appender = new ConsoleAppender()
		logger.addAppender(appender)
	}

	static main(args) {
	
	}

}
